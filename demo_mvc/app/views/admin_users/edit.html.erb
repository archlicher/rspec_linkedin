<% @page_title = "Edit user" %>

<%= link_to("<< Back to List", admin_users_path, :class => 'back_list')%>
<div class="users edit">
	<h2>Update User</h2>
	<%= form_for(@admin_user) do |f|%>

		<%= render(:partial => 'form', :locals => {:f => f}) %>

		<div class="form-buttons">
			<%= f.submit("Update User") %>
		</div>
	<% end %>
</div>
